{
  "name": "adlabore",
  "description": "A simple, maximally extensible, minimal-dependency framework for building modern Ethereum dApps in React.",
  "author": "FordBashirian",
  "repository": {
    "type": "git",
    "url": "https://github.com/RolandoSporer/adlabore.git"
  },
  "engines": {
    "node": "^10"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "publish:lerna": "lerna publish",
    "clean": "lerna clean --yes && lerna exec -- rm -rf dist/ .rts2_cache_{cjs,esm}/",
    "build": "lerna run build",
    "start": "yarn build && lerna run --parallel --no-bail start -- --noClean",
    "lint": "lerna run lint --parallel",
    "test": "lerna run test --parallel"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "lerna": "^3.18.3",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "tsdx": "^0.11.0"
  },
  "license": "GPL-3.0-or-later",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "bugs": {
    "url": "https://github.com/RolandoSporer/adlabore.git/issues"
  },
  "homepage": "https://github.com/RolandoSporer/adlabore.git",
  "version": "1.9.6",
  "dependencies": {
    "accusantiumut": "^1.0.0",
    "aperiamvoluptas": "^1.0.0",
    "eaqueaccusantium": "^1.0.0",
    "eaquequi": "^1.0.0",
    "omnisquod": "^1.1.0"
  }
}
